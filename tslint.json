{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "trailing-comma": [
            true, 
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "no-console": false,
        "variable-name": {
            "options": [
                "allow-snake-case",
                "allow-leading-underscore"
            ]
        }
    },
    "rulesDirectory": []
}